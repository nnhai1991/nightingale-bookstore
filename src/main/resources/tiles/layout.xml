<tiles-definitions>

	<!-- Master Template Page Admin -->
	<definition name="admin-template" template="/WEB-INF/views/template/master-admin.jsp">
		<put-attribute name="htmlhead" value="/WEB-INF/views/template/htmlhead-admin.jsp" />
		<put-attribute name="body" value="/WEB-INF/views/template/body.jsp" />
		<put-attribute name="footer" value="/WEB-INF/views/template/footer-admin.jsp" />
