	<!-- Master Template Page Normal --> 
	<definition name="default-template" template="/WEB-INF/views/template/master.jsp">
		<put-attribute name="htmlhead" value="/WEB-INF/views/template/htmlhead.jsp" /> 
    	<put-attribute name="body" value="/WEB-INF/views/template/body.jsp" />
    	<put-attribute name="footer" value="/WEB-INF/views/template/footer.jsp" />
	</definition>
	
</tiles-definitions>